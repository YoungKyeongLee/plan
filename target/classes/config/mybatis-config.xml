<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE configuration PUBLIC 
		"-//mybatis.org//DTD Config 3.0//EN" 
		"http://mybatis.org/dtd/mybatis-3-config.dtd">
		
<configuration>
	<settings>
		<setting name="cacheEnabled" value="false" />
		<setting name="useGeneratedKeys" value="false" />
		<setting name="mapUnderscoreToCamelCase" value="true"/>
	</settings>
	
	<typeAliases>
		<typeAlias type="com.young.vo.BucketVO" alias="bucket" />
		<typeAlias type="com.young.vo.BunchVO" alias="bunch" />
		<typeAlias type="com.young.vo.GoalVO" alias="goal" />
		<typeAlias type="com.young.vo.MembershipVO" alias="membership" />
		<typeAlias type="com.young.vo.ScheduleVO" alias="schedule" />
		
	</typeAliases>
	
</configuration>		